<launch>

<!-- Camera -->
  <include file="$(find openni2_launch)/launch/openni2.launch">
	<arg name="depth_registration" value="true" />
</include>

<!-- Object detection node-->
 <node pkg="object_detection" type="object_detection" name="object_detection" output="screen"
	respawn="true"/>

<!-- Object recognition node -->
 <node pkg="object_recognition" type="object_recognition" name="object_recognition" output="screen"
	respawn="true"/>

<!-- Speaker node -->
 <node pkg="ras_espeak" type="espeak_node" name="espeak" output="log"/>
</launch>
